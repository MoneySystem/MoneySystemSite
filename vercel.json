{
  "version": 2,
  "buildCommand": "NEXT_DISABLE_ESLINT=1 NEXT_DISABLE_TYPE_CHECKS=1 NEXT_TELEMETRY_DISABLED=1 NEXT_IGNORE_TYPE_ERROR=1 pnpm build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "typescript": {
          "ignoreBuildErrors": true
        },
        "eslint": {
          "ignoreDuringBuilds": true
        }
      }
    }
  ]
}